<title><%= siteTitle %></title>
<meta name="keywords" content="<%- themeConfig.siteKeywords %>" />
<meta name="description" content="<%- themeConfig.siteDescription %>" />
<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
<base href="<%= themeConfig.domain %>/">

<% let jsdelivr = "https://cdn.jsdelivr.net/gh/"+site.customConfig.username+"/"+site.customConfig.repository; %>
<link rel="shortcut icon" href="<%= jsdelivr %>/favicon.ico" />

<% if (site.customConfig.scheme === 'lens') {%>
<% if ( site.customConfig.jsdelivr == true) { %>
<link rel="stylesheet" href="<%= jsdelivr %>/media/css/main-lens.css" />
<noscript><link rel="stylesheet" href="<%= jsdelivr %>/media/css/noscript-lens.css" /></noscript>
<% } else{ %>
<link rel="stylesheet" href="media/css/main-lens.css" />
<noscript><link rel="stylesheet" href="media/css/noscript-lens.css" /></noscript>
<% }%>

<% } else {%>
<% if ( site.customConfig.jsdelivr == true) { %>
<link rel="stylesheet" href="<%= jsdelivr %>/media/css/tags.css" />
<link rel="stylesheet" href="<%= jsdelivr %>/media/css/main-multiverse.css" />
<link rel="stylesheet" href="<%= jsdelivr %>/media/css/modify-multiverse.css" />
<noscript><link rel="stylesheet" href="<%= jsdelivr %>/media/css/noscript-multiverse.css" /></noscript>
<% } else{ %>
<link rel="stylesheet" href="media/css/tags.css" />
<link rel="stylesheet" href="media/css/main-multiverse.css" />
<link rel="stylesheet" href="media/css/modify-multiverse.css" />
<noscript><link rel="stylesheet" href="media/css/noscript-multiverse.css" /></noscript>
<% }%>
<% } %>

<% if (site.customConfig.ga) { %>
<script async src="https://www.googletagmanager.com/gtag/js?id=<%= site.customConfig.ga %>"></script>
<script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', '<%= site.customConfig.ga %>');
</script>
<% } %>